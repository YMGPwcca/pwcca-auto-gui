{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devPath": "http://localhost:3000",
    "distDir": "../dist"
  },
  "package": {
    "productName": "pwcca-auto",
    "version": "0.0.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.ico",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "message": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "startDragging": true,
        "setAlwaysOnTop": true,
        "setPosition": true,
        "setSize": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "PwccaAuto for PwccaPC",
        "focus": false,
        "maximizable": false,
        "minimizable": false,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "ymg.pwcca.auto",
      "icon": [
        "icons/icon.ico"
      ]
    }
  }
}
{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "pwcca-auto",
    "version": "0.0.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.ico",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "message": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "startDragging": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "PwccaAuto for PwccaPC",
        "width": 450,
        "height": 900,
        "x": 1400,
        "y": 90,
        "focus": false,
        "maximizable": false,
        "minimizable": false,
        "resizable": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "ymg.pwcca.auto",
      "icon": [
        "icons/icon.ico"
      ]
    }
  }
}